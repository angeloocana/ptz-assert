{
  "name": "ptz-assert",
  "version": "1.6.3",
  "description": "ptz-assert is an assertion library for creating less verbose tests that wrappers npm assert",
  "main": "dist/index.js",
  "typings": "src/index.ts",
  "scripts": {
    "lint": "tslint ./src/*.ts ./src/**/*.ts ./src/**/*.test.ts ./src/**/*.d.ts",
    "js": "tsc",
    "pretest": "npm-run-all --parallel js lint",
    "mocha": "mocha ./dist/**/*.js --compilers js:babel-core/register --require babel-polyfill",
    "test": "nyc npm run mocha && nyc report --reporter=text-lcov > coverage.lcov && codecov --token=e8f9617b-0ab3-409e-afca-406d99c879a4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angeloocana/ptz-assert.git"
  },
  "keywords": [
    "polutz",
    "ptz",
    "ptz-assert",
    "assertion",
    "library"
  ],
  "author": "angeloocana.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angeloocana/ptz-assert/issues"
  },
  "homepage": "https://github.com/angeloocana/ptz-assert#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/sinon": "^2.1.2",
    "babel-core": "^6.24.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.18.0",
    "codecov": "^1.0.1",    
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^8.4.0",
    "sinon": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "assert": "^1.4.1"
  }
}
